@page
@model RazorPages.Pages.StudiosModel
@{
    ViewData["Title"] = "Studios";
}

<body>
<br>
<br>
<br>
<br>

<h4>Studio: </h4>

<div class="row">
    @foreach (var item in @Model.Studios)
    {
        
        <div class="column">
            <p>ID: @item.Id</p>
            <p>Name: @item.Name</p>
            <p>City: @item.City</p>
        </div>

    }
</div>



</body>
<style>
* {
  box-sizing: border-box;
}

.column {
  float: left;
  width: 33.33%;
  padding: 5px;
}
.row::after {
  content: "";
  clear: both;
  display: table;
}
</style>